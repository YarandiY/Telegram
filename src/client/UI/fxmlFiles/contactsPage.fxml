<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>


<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane  maxHeight="-Infinity" fx:id="pane"
             maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
             prefHeight="600.0" prefWidth="450.0"
             style="-fx-background-color: white"
             stylesheets="@../cssFiles/contactsPage.css" xmlns="http://javafx.com/javafx/9.0.1"
             xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.ContactsPage">
<VBox fx:id="mainVBox" prefHeight="600.0" prefWidth="450.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: white">
    <HBox style=" -fx-background-color: white ; -fx-alignment: center" spacing="20">
        <Button fx:id="menuButton" onAction="#menuPage">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../materialFiles/icons/menu2.png"></Image>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <TextField promptText="Search" minWidth="360" fx:id="searchField"></TextField>
    </HBox>
</VBox>
</AnchorPane>
